<Project Sdk="Microsoft.NET.Sdk">

  <ItemGroup>
    <ProjectReference Include="..\AsciiDoc.NET.Core\AsciiDoc.Net.Core.csproj" />
    <ProjectReference Include="..\AsciiDoc.NET.Parser\AsciiDoc.Net.Parser.csproj" />
    <ProjectReference Include="..\AsciiDoc.NET.Converters.Core\AsciiDoc.Net.Converters.Core.csproj" />
    <ProjectReference Include="..\AsciiDoc.NET.Converters.Html\AsciiDoc.Net.Converters.Html.csproj" />
  </ItemGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <PackageId>AsciiDoc.Net</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <Authors>AsciiDoc.Net Team</Authors>
    <Company>AsciiDoc.Net</Company>
    <Product>AsciiDoc.Net</Product>
    <Description>AsciiDoc.Net - A comprehensive .NET library for parsing AsciiDoc documents and converting them to various output formats. This is the main facade package with high-level APIs.</Description>
    <PackageTags>asciidoc;documentation;markup;parser;converter;dotnet</PackageTags>
    <PackageProjectUrl>https://github.com/AsciiDoc-Net/AsciiDoc.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AsciiDoc-Net/AsciiDoc.Net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>



</Project>
